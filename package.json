{
  "name": "rowma_js",
  "version": "1.1.15",
  "description": "Rowma SDK for JavaScript/Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "test": "mocha --require ts-node/esm --watch-extensions ts 'test/**/*.ts'",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src/*.ts test",
    "build": "tsc",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "esdoc": "esdoc"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rowma/rowma.git"
  },
  "author": "asmsuechan <suenagaryoutaabc@gmail.com>",
  "license": "mit",
  "bugs": {
    "url": "https://github.com/rowma/rowma/issues"
  },
  "homepage": "https://github.com/rowma/rowma",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "chai": "^4.3.6",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^9.2.2",
    "nyc": "^13.3.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "socket.io-client": "^2.2.0",
    "socket.io-parser": "^4.1.2",
    "uuid": "^8.3.2"
  }
}
